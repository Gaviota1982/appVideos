<!DOCTYPE html>
<html lang="es">
  <head>
    <%- include('./html/head');%>
    <title>Crear y modificar un JSON</title>
  </head>
  <body>
    <%- include('./html/navegacion');%>
    <div class="conteiner p-4">
      <div class="row">
        <% arrVideos.forEach( function(video){ %>
        <div class="col-md-4">
          <div class="card">
            <div class="card-header">
              <p><%= video.title; %></p>
            </div>
            <div class="card-body">
              <p><%= video.autor; %></p>
            </div>
            <div class="card-footer">
                <a href="/delete/<%= video.id %>" class="btn btn-denger">
                    Borrar
                </a>
            </div>
          </div>
        </div>
        <% }); %>
      </div>
    </div>
  </body>
</html>
